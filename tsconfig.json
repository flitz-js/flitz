{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "lf",
    "noUnusedLocals": true,
    "outDir": "lib",
    "pretty": false,
    "removeComments": false,
    "resolveJsonModule": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "es2017"
  }
}
